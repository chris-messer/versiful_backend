# E2E Test Configuration
# Copy this to .env and fill in your values

# Required for API endpoint E2E tests
API_BASE_URL=https://api.dev.versiful.io

# Optional: for Lambda invoke E2E tests
LAMBDA_WEB_NAME=dev-versiful-web_function
LAMBDA_SMS_NAME=dev-sms_function

# Optional: for authenticated endpoint tests (get from browser dev tools after login)
TEST_AUTH_TOKEN=

# Optional: enable SMS E2E test (will send real SMS)
ALLOW_SMS_E2E=false
